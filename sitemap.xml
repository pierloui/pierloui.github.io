---
permalink: sitemap.xml/
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">


  <url>
    <loc>{{ site.baseurl }}/"</loc>
  </url>


  {% for post in site.posts %}
  <url>
    <loc>{{ post.url | prepend: site.baseurl }}</loc>
  </url>
  {% endfor %}

</urlset>


